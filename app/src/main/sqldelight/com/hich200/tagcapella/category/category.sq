selectAll:
SELECT * FROM tag_category;

selectCategoryById:
SELECT * FROM tag_category WHERE id = ?;

insertCategory:
INSERT INTO tag_category (category) VALUES (?);

lastInsertedCategory:
SELECT * FROM tag_category ORDER BY id DESC LIMIT 1;

updateCategory:
UPDATE tag_category SET category = ? WHERE id = ?;

deleteCategory:
DELETE FROM tag_category WHERE id = ?;

selectTagsInCategory:
SELECT * FROM tag WHERE category = ?;

