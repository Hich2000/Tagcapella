selectSongTags:
SELECT tag.id, tag.tag, tag.category FROM song_tag JOIN tag ON song_tag.tag = tag.id WHERE song = ?;

filterSongList:
SELECT s.id, s.song FROM song_tag s
WHERE s.tag IN :includeTags
GROUP BY s.song;

addSongTag:
INSERT INTO song_tag (song, tag) VALUES (?, ?);

deleteSongTag:
DELETE FROM song_tag WHERE tag = ? AND song = ?;
